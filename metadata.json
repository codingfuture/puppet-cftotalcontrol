{
  "name": "codingfuture-cftotalcontrol",
  "version": "0.10.0",
  "author": "Andrey Galkin",
  "summary": "Special Bash environment for total control of configured Puppet hosts through SSH",
  "license": "Apache-2.0",
  "source": "https://github.com/codingfuture/puppet-cftotalcontrol",
  "project_page": "https://github.com/codingfuture/puppet-cftotalcontrol",
  "issues_url": "https://github.com/codingfuture/puppet-cftotalcontrol/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "15.10",
        "16.04",
        "16.10"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.8.0 <5.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 2.3.0 <3.0.0"
    },
    {
      "name": "codingfuture-cfnetwork",
      "version_requirement": ">= 0.10.1 <0.11.0"
    },
    {
      "name": "codingfuture-cfauth",
      "version_requirement": ">= 0.10.1 <0.11.0"
    }
  ],
  "data_provider": "hiera"
}
